From 177d9fe942059947415295340963c02ae51c48ff Mon Sep 17 00:00:00 2001
From: Daniele Di Sarli <danieleds0@gmail.com>
Date: Mon, 17 Sep 2018 20:31:13 +0200
Subject: [PATCH 03/13] fix: give priority to the scalable resolution icon

---
 src/ui/iconprovider.cpp | 10 ++++++----
 1 file changed, 6 insertions(+), 4 deletions(-)

diff --git a/src/ui/iconprovider.cpp b/src/ui/iconprovider.cpp
index 341ffda82..71e82418b 100644
--- a/src/ui/iconprovider.cpp
+++ b/src/ui/iconprovider.cpp
@@ -1,6 +1,7 @@
 #include "include/iconprovider.h"
 
 #include <QDebug>
+#include <QFileInfo>
 
 IconProvider::IconProvider()
 {
@@ -24,15 +25,16 @@ QIcon IconProvider::fromTheme(const QString &name)
                     QPair<int, QString>(128, "128x128"),
         };
 
+        if (QFileInfo(basePath.arg("scalable").arg(name).arg("svg")).exists()) {
+            icon.addFile(basePath.arg("scalable").arg(name).arg("svg"),
+                         QSize(512, 512));
+        }
+
         for (const QPair<int, QString> & size : sizes) {
             icon.addFile(basePath.arg(size.second).arg(name).arg("png"),
                          QSize(size.first, size.first));
-
         }
 
-        icon.addFile(basePath.arg("scalable").arg(name).arg("svg"),
-                     QSize(512, 512));
-
         // Warn about missing icons.
         // This works also as a workaround for this Qt bug in macOS: https://bugreports.qt.io/browse/QTBUG-58344
         if (icon.pixmap(1, 1).isNull()) {

